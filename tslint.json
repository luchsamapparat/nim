{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "array-type": false,
        "arrow-parens": false,
        "interface-name": false,
        "max-classes-per-file": false,
        "max-line-length": false,
        "object-literal-sort-keys": false,
        "quotemark": [true, "single", "avoid-escape"],
        "trailing-comma": false
    },
    "rulesDirectory": []
}